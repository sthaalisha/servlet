<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>SimpleServlet</servlet-name>
        <servlet-class>basicservlet.SimpleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SimpleServlet</servlet-name>
        <url-pattern>/web</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GenServlet</servlet-name>
        <servlet-class>basicservlet.GenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GenServlet</servlet-name>
        <url-pattern>/second</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FirstServlet</servlet-name>
        <servlet-class>basicservlet.FirstServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstServlet</servlet-name>
        <url-pattern>/first</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HandleRequest</servlet-name>
        <servlet-class>basicservlet.HandleRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HandleRequest</servlet-name>
        <url-pattern>/handle</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>cookieDemo.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/ser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>cookieDemo.SecondServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/serv</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FirstPage</servlet-name>
        <servlet-class>SessionDemo.FirstPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FirstPage</servlet-name>
        <url-pattern>/Fir</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SecondPage</servlet-name>
        <servlet-class>SessionDemo.SecondPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SecondPage</servlet-name>
        <url-pattern>/Sec</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProcessServlet</servlet-name>
        <servlet-class>DatabaseServlet.ProcessServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProcessServlet</servlet-name>
        <url-pattern>/ProcessServlet</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>authenticate</servlet-name>
        <servlet-class>Dispatcher.authenticate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>authenticate</servlet-name>
        <url-pattern>/authenticate</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>welcome</servlet-name>
        <servlet-class>Dispatcher.welcome</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>welcome</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
</web-app>
